//匹配验证码
Function match(index)
	Dim colorArr
	colorArr = Array(Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0),Array(0,0,0,0,0,0,0,0,0))
	
	Dim offests
	offests = index * 9
	If index = 2 Then
		offests = offests + 2
	End If
	
	If index = 3 Then 
		offests = offests + 2
	End If
	
	For i = 0 To 8
		tmpArr = Array(0,0,0,0,0,0,0,0,0)
		For j = 0 To 8
			GetColor=GetPixelColor(X +offsetX+j + offests,Y+offsetY+i)
			
			If RevRGB(GetColor) < v2 Then 
				tmpArr(j) = 1
			End If
			//str = str + "(" + CStr(X + offsetX + i) + "," + CStr(Y + offsetY + j) + ":" + CSTr(GetColor) + "),"
			//MsgBox str
			//str = str +"("+  CSTr(GetColor) + "-" +  CStr(RevRGB(GetColor))  + " " + CSTr( v2 )+"),"
			str = str + CStr(tmpArr(j)) + ","
		Next
		colorArr(i) = tmpArr
		//TracePrint str
		
		str = ""
	Next
	
	
	Dim rs
	Dim wrong
	
	rs = ""
	
	Dim vl, vls, result, resultWrong
	vl = ""
	result = 0
	resultWrong = 64
	
	For index = 0 To 9
		tmpVl = ""
		vl = "Array("
		wrong = 0
		For i = 0 To 8
			vl = vl + "),Array("
			For j = 0 To 8
				//vl = vl+CStr(colorArr(i)(j)) + ","
				//tmpVl =tmpVl+ CStr(cs(index)(i)(j)) + ","
 			
				If CInt(colorArr(i)(j)) <> CInt(cs(index)(i)(j)) Then 
					wrong = wrong + 1
					//TracePrint CSTr(colorArr(i)(j)) + "-" + CSTR(cs(index)(i)(j)) + "-" + CSTR(wrong)
				End If
			Next
			
			//vl = ")" + vl
			//tmpVl = tmpVl + "   "
		Next
		
		//TracePrint "数字" + CSTR(index) + ":   " + vbLf+tmpVl
		
		//rs = rs + CSTr(index) + "," + CStr(wrong) + vbLf
		
		If wrong < resultWrong Then 
			result = index
			resultWrong = wrong
		End If
	Next
	
	match = result
		
End Function

Function RevRGB(colors)
	Call Plugin.Color.ColorToRGB(CStr(colors), red, green, blue) 
    RevRGB= CLng(blue + (green * 256) + (red * 65536))
End Function

