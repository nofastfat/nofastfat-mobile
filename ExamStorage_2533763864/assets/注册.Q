	Delay 100
	
	Call Plugin.Web.HtmlInput(uName, "id:txtNickName")
	Delay 100
	
	Call Plugin.Web.HtmlInput(pass, "id:txtLoginPass")
	Delay 100
	
	Call Plugin.Web.HtmlInput(pass, "id:txtloginConPass")
	Delay 100
	
	Call Plugin.Web.HtmlInput(pass, "id:txtInsurePass")
	Delay 100
	
	Call Plugin.Web.HtmlInput(pass, "id:txtInsureConPass")
	Delay 100
	
	Call Plugin.Web.HtmlInput(coreCode, "id:txtVerifyCode")
	Delay 100
End Function

