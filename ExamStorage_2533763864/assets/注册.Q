//随机ID
Function makeRandomId()
	Dim id
	id = ""
	
	For 15
		Randomize
		If Rnd > 0.5 Then 
			id = id + CStr(Int((9 * Rnd) + 0))
		Else 
			id = id + chr(Int((26 * Rnd) + 97))
		End If
	Next
	
	
	makeRandomId = id
End Function

Function makeRandomPass()
	Dim pass
	pass = ""
	
	For 6
		Randomize
		If Rnd > 0.5 Then 
			pass = pass + CStr(Int((9 * Rnd) + 0))
		Else 
			pass = pass + chr(Int((26 * Rnd) + 97))
		End If
	Next
	
	
	makeRandomPass = pass
End Function


